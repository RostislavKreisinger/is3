{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.2.*",
        "barryvdh/laravel-ide-helper": "^2.1",
        "monkey/vardump": "^1.2.0",
        "monkey/environment": "^1.3.0",
        "doctrine/dbal": "^2.5",
        "latte/latte": "^2.3",
        "monkey/translator": "^1.1.0",
        "monkey/old-monkey": "^1.0",
        "monkey/crypt": "^1.0",
        "monkey/config": "^1.0",
        "monkey/url-handler": "^1.0",
        "monkey/connections": "^2.4"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.8",
        "symfony/css-selector": "2.8.*|3.0.*",
        "symfony/dom-crawler": "2.8.*|3.0.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "": "helpers/"
        },
        "files": [
            "app/helpers.php",
            "vendor/monkey/old-monkey/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "rm bootstrap/cache/compiled.php -f"
        ],
        "post-update-cmd": [
            "rm bootstrap/cache/compiled.php -f"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://composer:uxJ4EhTaf61fo3pnTvK5@composer.monkeydata.com"
        }
    ]
}
