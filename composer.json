{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.2.*",
        "barryvdh/laravel-ide-helper": "^2.1",
        "monkey/vardump": "^1.2.0",
        "monkey/environment": "^1.5.0",
        "doctrine/dbal": "^2.5",
        "latte/latte": "^2.3",
        "monkey/translator": "dev-feature/com-48-translation-to-jira as 2.3.1",
        "monkey/old-monkey": "^1.0",
        "monkey/crypt": "^1.0",
        "monkey/config": "dev-feature/com-57-jira-credentials as 1.24.0",
        "monkey/url-handler": "^1.1.0",
        "monkey/connections": "^2.6.0",
        "monkey/raven": "^2.0.0",
        "monkey/constants": "^1.1.0",
        "monkey/import-eshop-sdk": "^1.1.0",
        "monkey/import-eshop-data-objects": "^1.1.0",
        "nilportugues/serializer": "^1.4.0",
        "monkey/oauth2-client": "^1.0.1",
        "monkey/oauth2-api": "^1.2.2"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.8",
        "symfony/css-selector": "2.8.*|3.0.*",
        "symfony/dom-crawler": "2.8.*|3.0.*",
        "monkey/jira-sdk": "dev-feature/com-48-jira-sdk as 1.0.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "": "helpers/"
        },
        "files": [
            "app/helpers.php",
            "vendor/monkey/old-monkey/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "rm bootstrap/cache/compiled.php -f"
        ],
        "post-update-cmd": [
            "rm bootstrap/cache/compiled.php -f"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "platform": {
            "php": "7.0"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://composer.monkeydata.com"
        }
    ]
}
